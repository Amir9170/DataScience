   "source": [
    "from numpy import random\n",
    "\n",
    "i = int(input('Please Enter a Number : '))\n",
    "x = random.randint(100, size=(i))\n",
    "biggest_number = x.max()\n",
    "print ('Random Numbers are :', x)\n",
    "print ('The Biggest Number is :', biggest_number)\n",
    "if biggest_number > 70 :\n",
    "    print ('WIN')\n",
    "else:\n",
    "    print ('FAIL')"
   ]
